    "nanoid": "^5.1.5",
    "next": "^14.1.0",
    "next-pwa": "^5.6.0",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
